<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewData["Title"] | Helperland</title>
    <link rel="shortcut icon" href="/img/favicon_img.png" type="image/x-icon">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
        integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
     @RenderSection("css",required:false)
</head>
<body>
    <header class="price-navbar">
        <nav class="prices-nav navbar navbar-expand-lg navbar-dark p-0 fixed-top " id="SecondaryNav">
            <div class="container-fluid">
                <a class="navbar-brand px-15" asp-controller="Home" asp-action="Index"><svg xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink" width="73" height="54">
                        <image width="73" height="54"
                            xlink:href="data:img/png;base64," />
                    </svg></a>
                <button class="navbar-toggler text-light" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ms-auto mb-2 mb-lg-0 price-navMenu text-center">
                        @if(@ViewBag.UType == 1)
                        {   
                            <li class="nav-item">
                                <a class="price-sp-btn header-nav" asp-controller="Customer" asp-action="Bookservice">Book now</a>
                            </li>
                        }
                        <li class="nav-item">
                            <a class=" header-nav" asp-controller="Home" asp-action="Price">Prices & services</a>
                        </li>
                        <li class="nav-item">
                            <a class=" header-nav">Warranty</a>
                        </li>
                        <li class="nav-item">
                            <a class=" header-nav">Blog</a>
                        </li>
                        <li class="nav-item">
                            <a class=" header-nav" asp-controller="Home" asp-action="Contact">Contact</a>
                        </li>
                        @if(@ViewBag.IsloggedIn == "success")
                        {
                            <li class="nav-item us-notification">
                                <a class="m-2"><i class="fas fa-bell fa-lg"></i> <span
                                        class="notify-badge">1</span></a>
                            </li>
                            <li class="nav-item dropdown us-user">
                                <a class="" data-bs-toggle="dropdown" aria-expanded="false" id="navbarDarkDropdownMenuLink">
                                    <i class="far fa-user-circle fa-lg"></i>
                                    <i class="fas fa-angle-down fa-lg ps-2"></i>
                                </a>
                                <ul class="dropdown-menu dropdown-menu-end text-start us-user-dropdown"
                                    aria-labelledby="navbarDarkDropdownMenuLink">
                                    <li><label class="dropdown-item text-dark">Welcome,<br><strong>@ViewBag.Uname</strong></label></li>
                                    <li>
                                        <hr class="dropdown-divider">
                                    </li>
                                    <li><a class="dropdown-item text-dark" asp-controller="Customer" asp-action="Dashboard">My Dashboard</a></li>
                                    <li><a class="dropdown-item text-dark" asp-action="Myaccount" asp-controller="Customer">My Settings</a></li>
                                    <li><a class="dropdown-item text-dark" asp-controller="Login" asp-action="Logout">Logout</a></li>
                                </ul>
                            </li>
                        }else
                        {
                            <li class="nav-item">
                                <a class="price-sp-btn header-nav" asp-controller="Home" asp-action="Index" asp-route-login="true">Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="price-sp-btn header-nav" asp-controller="Login" asp-action="BecomePro">Become a Helper</a>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </nav>      
        
        <!-- Mobile View Navbar Start -->
        <nav class="shSmallNav serviceHistory-nav fixed-top" id="smHomeNav">
            <div class="container-fluid">
                <div class="smallNavlogo">
                    <a asp-action="Index" asp-controller="Home"><img src="/img/logo-small.png" alt=""></a>
                </div>
                @if (@ViewBag.IsloggedIn == "success")
                {
                    <div class="smallNavNofitify">
                        <a class="m-2" href="#"><i class="fas fa-bell fa-lg"></i> <span class="sm-notify-badge">1</span></a>
                    </div>
                }
                <div class="hamburger">
                    <i class="fa fa-bars"></i>
                </div>
                <div class="smNavMenu d-flex">
                    <div class="firstPart"></div>
                    <div class="secondPart">
                        @if (@ViewBag.IsloggedIn == "success")
                        {
                            <div class="uName">
                                Welcome,<br>
                                <strong>@ViewBag.Uname</strong>
                            </div>
                            <hr class="smNavSeparator">
                            <div class="nav flex-column text-start dashNavSide">
                                @if(@ViewBag.UType == 1){
                                    <a asp-controller="Customer" asp-action="Dashboard">Dashboard</a>
                                    <a asp-controller="Customer" asp-action="Servicerequest">Service History</a>
                                    <a>Service Schedule</a>
                                    <a>Favourite Pros</a>
                                    <a>Invoices</a>
                                    <a>Notifications</a>
                                }else if(@ViewBag.UType == 2)
                                {
                                    <a>Dashboard</a>
                                    <a>New Service Requests</a>
                                    <a>Upcoming services</a>
                                    <a>Service Schedule</a>
                                    <a>Service History</a>
                                    <a>My Ratings</a>
                                    <a>Block Customer</a>
                                    <a>Invoices</a>
                                    <a>Notifications</a>
                                }
                            </div>
                            <div class="navItem"><a asp-action="Myaccount" asp-controller="Customer"> My Settings</a></div>
                            <div class="navItem"><a asp-controller="Login" asp-action="Logout"> Logout</a></div>
                            <hr class="smNavSeparator">
                            @if (@ViewBag.UType == 1)
                            {
                                <div class="navItem"><a asp-controller="Customer" asp-action="Bookservice">Book Now</a></div>
                            }
                            <div class="navItem"><a asp-controller="Home" asp-action="Price">Plans & Services</a></div>
                            <div class="navItem"><a>Warranty</a></div>
                            <div class="navItem"><a>Blog</a></div>
                            <div class="navItem"><a asp-controller="Home" asp-action="Contact">Contact</a></div>
                            <hr class="smNavSeparator">
                            <div class="d-flex justify-content-center align-items-center  socialIcon">
                                <a href="#"><i class="fab fa-facebook-f fa-2x"></i></a>
                                <a href="#"><i class="fab fa-instagram fa-2x"></i></a>
                            </div>
                        }else{
                            @if (@ViewBag.UType == 1)
                            {
                                <div class="navItem"><a asp-controller="Customer" asp-action="Bookservice">Book Now</a></div>
                            }
                            <div class="navItem"><a asp-controller="Home" asp-action="Price">Plans & Services</a></div>
                            <div class="navItem"><a>Warranty</a></div>
                            <div class="navItem"><a>Blog</a></div>
                            <div class="navItem"><a asp-controller="Home" asp-action="Contact">Contact</a></div>
                            <div class="navItem"><a asp-controller="Home" asp-action="Index" asp-route-login="true">Login</a></div>
                            <div class="navItem"><a asp-controller="Login" asp-action="BecomePro">Become a Helper</a></div>
                            <hr class="smNavSeparator">
                            <div class="d-flex justify-content-center align-items-center  socialIcon">
                                <a href="#"><i class="fab fa-facebook-f fa-2x"></i></a>
                                <a href="#"><i class="fab fa-instagram fa-2x"></i></a>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </nav>
        <!-- Mobile View Navbar End -->
    </header>
    <div>
        @RenderBody()
    </div>
    
    <!-- Footer Start -->
   <footer class="footer">
        <div class="container">
            <div class="row pt-3">
                <div class="col-lg-2 text-center p-0">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="120"
                        height="92">
                        <image width="120" height="92"
                            xlink:href="data:img/png;base64," />
                    </svg>
                </div>
                <div class="col-lg-8 d-flex justify-content-center align-items-center">
                    <div class="footer-navbar text-center p-4 ">
                        <a class="" asp-controller="Home" asp-action="Index">HOME</a>
                        <a class="" asp-controller="Home" asp-action="About">ABOUT</a>
                        <a class="">TESTIMONIALS</a>
                        <a class="" asp-controller="Home" asp-action="Faq">FAQS</a>
                        <a class="">INSURANCE</a>
                        <a class="">POLICY</a>
                        <a class="">IMPRESSUM</a>
                    </div>
                </div>
                <div class="col-lg-2 py-4 d-flex justify-content-center align-items-center">
                    <a href="#"><img src="/img/fb.svg" alt="fb" class="px-2"></a>
                    <a href="#"><img src="/img/insta.svg" alt="insta" class="px-2"></a>
                </div>
            </div>
            <div class="row ">
                <div class="col-lg-12 p-0">
                    <hr class="footer-separator">
                </div>
            </div>
            <div class="row text-center">
                <p class="footer-policy"> &copy; 2018 Helperland. All rights reserved. Terms and Conditions | Privacy Policy</p>
            </div>
        </div>
    </footer>
    <!-- Footer End -->

    @if(@ViewBag.Page == "ResetPsw"){

    }
    else
    {
        <!-- Back-Home Button -->
        <div class="home-btn" id="homebtn">
            <a>
                <div class="up-arrow">
                    <svg class="up-pos" xmlns="http://www.w3.org/2000/svg" width="20" height="10">
                        <path fill-rule="evenodd" fill="#F4F5F8"
                                d="M19.802 8.855c.132.129.197.29.197.466a.633.633 0 0 1-.197.466c-.262.653-.689.653-.951 0l-8.365-8.195L2.12 9.787c-.262.653-.689.653-.951 0a.65.65 0 0 1 0-.932L10.1.195c.172.425.599.425.861 0l8.841 8.66z" />
                    </svg>
                </div>
            </a>
        </div>
        <!-- Back-Home Button -->
    }
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="/js/bootstrap.bundle.min.js"></script>
    @RenderSection("script", required:false)
</body>
</html>
